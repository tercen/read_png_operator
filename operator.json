{
  "name": "PNG",
  "description": "Imports a PNG file.",
  "tags": [
    "import"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/read_png_operator"
  ],
  "properties": [],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "1.0.0",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "documentId",
            "type": "",
            "description": "Document ID of the PNG file to read",
            "ontologyMapping": "document",
            "crosstabMapping": "column",
            "cardinality": "1..n",
            "factors": []
          }
        ],
        "axis": []
      }
    ],
    "outputSpecsV2": [
      {
        "kind": "OperatorJoinSpec",
        "joinOperators": [
          {
            "kind": "JoinOperator",
            "joinType": "left",
            "leftPair": {
              "kind": "ColumnPair",
              "lColumns": [
                "documentId"
              ],
              "rColumns": [
                "documentId"
              ]
            },
            "rightRelation": {
              "kind": "TableRelation",
              "attributes": [
                {
                  "kind": "Attribute",
                  "name": "row_id",
                  "type": "string"
                },
                {
                  "kind": "Attribute",
                  "name": "col_id",
                  "type": "string"
                },
                {
                  "kind": "Attribute",
                  "name": "pixel_value",
                  "type": "double"
                }
              ]
            }
          }
        ]
      }
    ]
  }
}